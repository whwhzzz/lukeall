<?xml version="1.0" encoding="ISO-8859-1"?>
<dialog text=" Explanation" name="explain" icon="/img/luke.gif" modal="true" columns="1" top="8" left="8" 
        bottom="8" right="8" gap="15" resizable="true" close="remove(explain)">
	<label icon="/img/info.gif" text="Explanation of the document hit:"/>
	<tree name="eTree" halign="fill" valign="fill" weightx="1" weighty="1" line="false" angle="true" width="400" height="150"/>
	<panel halign="right" gap="8">
	    <button text="Copy" halign="center" action="clipExplain(explain)"/>
	    <button type="default" text=" OK " action="remove(explain)"/>
	</panel>
</dialog>
